<html>
<head>
    <style>
        .button {
            background-color: #4CAF50; /* Green */
            border: none;
            color: white;
            padding: 15px 32px;
            text-align: center;
            text-decoration: none;
            display: inline-block;
            margin: 4px 2px;
            cursor: pointer;
        }

        .button1 {
            font-size: 40px;
        }
    </style>
    <!-- for-mobile-apps -->
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-state=1" />
    <link type="text/css" rel="stylesheet" href="css/bootstrap.css" />
    <script src="js/jquery-1.1.1.min.js"></script>
    <script src="js/bootsrap.js"></script>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta name="keywords" content="Best Store Responsive web template, Bootstrap Web Templates, Flat Web Templates, Android Compatible web template,
Smartphone Compatible web template, free webdesigns for Nokia, Samsung, LG, SonyEricsson, Motorola web design" />
    <script type="application/x-javascript">
        addEventListener("load", function() { setTimeout(hideURLbar, 0); }, false);
               function hideURLbar(){ window.scrollTo(0,1); } </script>
    <!-- //for-mobile-apps -->
    <link href="~/abc/css/bootstrap.css" rel="stylesheet" type="text/css" media="all" />
    <link href="~/abc/css/style.css" rel="stylesheet" type="text/css" media="all" />
    <!-- js -->
    <script src="~/abc/js/jquery.min.js"></script>
    <!-- //js -->
    <!-- cart -->
    <script src="~/abc/js/simpleCart.min.js"> </script>
    <!-- cart -->
    <link rel="stylesheet" type="text/css" href="~/abc/css/jquery-ui.css">
    <!-- for bootstrap working -->
    <script type="text/javascript" src="~/abc/js/bootstrap-3.1.1.min.js"></script>
    <!-- //for bootstrap working -->
    <link href='//fonts.googleapis.com/css?family=Open+Sans:400,300,300italic,400italic,600,600italic,700,700italic,800,800italic' rel='stylesheet' type='text/css'>
    <link href='//fonts.googleapis.com/css?family=Lato:400,100,100italic,300,300italic,400italic,700,700italic,900,900italic' rel='stylesheet' type='text/css'>
    <!-- animation-effect -->
    <link href="~/abc/css/animate.min.css" rel="stylesheet">
    <script src="~/abc/js/wow.min.js"></script>
    <script>
        new WOW().init();
    </script>
    <style>
        table, td, th {
            border: 1px solid #ddd;
            text-align: left;
        }

        table {
            border-collapse: collapse;
            width: 100%;
        }

        th, td {
            padding: 15px;
        }
    </style>
    <!-- //animation-effect -->
</head>

<body>
    <!-- header -->
    <div class="header">
        <div class="container">
            <div class="header-grid">
                <div class="header-grid-left animated wow slideInLeft" data-wow-delay=".5s">
                    <ul>
                        <li><i class="glyphicon glyphicon-log-in" aria-hidden="true"></i><a href="">Hey Admin ... !!</a></li>

                    </ul>
                </div>
            </div>
            <div class="logo-nav">
                <div class="logo-nav-left animated wow zoomIn" data-wow-delay=".5s">
                    <h1><a href="~/My/adm">Buyit <span>Shop anywhere</span></a></h1>
                </div>
            </div>
            <div class="logo-nav-right">
                <div class="search-box">
                    <div id="sb-search" class="sb-search">
                        <form action="/My/searchresult" method="get">
                            <input class="sb-search-input" placeholder="Enter your search term..." type="search" id="search">
                            <input class="sb-search-submit" type="submit" value="">
                            <span class="sb-icon-search"> </span>
                        </form>
                    </div>
                </div>
                <!-- search-scripts -->
                <script src="~/abc/js/classie.js"></script>
                <script src="~/abc/js/uisearch.js"></script>
                <script>
                    new UISearch(document.getElementById('sb-search'));
                </script>
                <!-- //search-scripts -->
                <div class="clearfix"> </div>
            </div>
        </div>
    </div>
    <!-- //header -->
    <div style=" padding-right: 50px; ">
        <br /><br />
        <table>
            <tr>
                <th>Name</th>
                <th>Email </th>
                <th>Password</th>
                <th></th>
            </tr>
            @foreach (var s in Model)
            {
                <tr>
                    <td>@s.fname  @s.lname</td>
                    <td>@s.Email</td>
                    <td>@s.Password</td>
                    <td><a href="@Url.Action("del", "My", new { Email = @s.Email})">Delete</a></td>
                </tr>

            }

        </table>
    </div>
        <br />
        <br>
        <br>



        <!-- footer -->
        <div class="footer">
            <div class="container">

                <div class="footer-logo animated wow slideInUp" data-wow-delay=".5s">
                    <h2><a href="~/My/adm">Best Store <span>shop anywhere</span></a></h2>
                </div>

            </div>
        </div>
        <!-- //footer -->
</body>
</html>


